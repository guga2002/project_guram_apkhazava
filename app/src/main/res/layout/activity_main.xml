<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/narinj2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    app:layout_constraintBottom_toTopOf="@+id/gilaki"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.626"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.769"

    <TextView
        android:id="@+id/teqsti"
        android:layout_width="900px"
        android:layout_height="wrap_content"
        android:background="@color/shesvla"
        android:ems="10"
        android:fontFamily="sans-serif-light"
        android:inputType="textPersonName"
        android:text="enter your name"
        android:textSize="100px"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="387dp" />

    <Button
        android:id="@+id/gilaki"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="60dp"
        android:onClick="onButtonClicked"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/login"
        android:layout_width="600px"
        android:layout_height="100px"
        android:fontFamily="serif"
        android:text="welcome ,Login"
        android:textSize="80px"
        android:background="@color/lurji"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="80dp"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="289dp"
        android:layout_height="206dp"
        app:srcCompat="@android:drawable/ic_menu_mapmode"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="150dp"
        android:background="@color/narinj2"
        tools:ignore="MissingConstraints" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="800px"
        android:layout_height="100px"
        android:background="@color/shesvla"
        android:fontFamily="sans-serif"
        android:text="terms and conditions"
        android:textSize="70px"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="549dp" />

    <Button
        android:id="@+id/meoregverdzegadasvla"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="next"
        android:background="@color/narinjisferi"
        tools:layout_editor_absoluteX="302dp"
        tools:layout_editor_absoluteY="24dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="900px"
        android:layout_height="150px"
        android:ems="10"
        android:fontFamily="sans-serif-light"
        android:background="@color/shesvla"
        android:inputType="textEmailAddress"
        android:text="enter your gmail"
        android:textSize="90px"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="458dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>
